<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:c="http://www.springframework.org/schema/c" xmlns:p="http://www.springframework.org/schema/p"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean name="cqSR" class="top.yzlin.cqrobotsdk.Lemoc" c:port="${SR.port}"/>

    <bean id="srModian" class="top.yzlin.yyh.SRModian"
          c:cqRobot-ref="cqSR"
          c:groupID="${SR.groupID}"
          c:proID="${SR.proID}"/>

    <bean class="top.yzlin.cqrobotsdk.RereadMachine"
          c:cqRobot-ref="cqSR"
          c:gid="${SR.groupID}"/>

    <bean id="srWeiBo" class="top.yzlin.weibo.WeiBoMonitoring"
          c:cqRobot-ref="cqSR"
          c:groupID="${SR.groupID}">
        <constructor-arg name="weiBo">
            <bean class="top.yzlin.weibo.WeiBo"
                  c:name="孙芮"
                  c:uid="3669091483"
                  c:verifier="7a3b937f"/>
        </constructor-arg>
    </bean>

    <bean id="srKD" class="top.yzlin.koudai48.KDRoomMonitoring"
          c:groupID="${SR.groupID}"
          c:cqRobot-ref="cqSR">
        <constructor-arg name="kdRoom">
            <bean class="top.yzlin.koudai48.KDRoom"
                  c:memberName="孙芮"
                  c:kdValidation-ref="kdValidation"
                  p:faiPaiOpen="false"
                  p:idolFlipOpen="false"/>
        </constructor-arg>
    </bean>

    <bean class="top.yzlin.cqrobotsdk.keywordfunc.KeyWord"
          c:groupID="${SR.groupID}"
          c:cqRobot-ref="cqSR">
        <constructor-arg name="admin">
            <list value-type="java.lang.String">
                <value>499680328</value>
                <value>1187781378</value>
                <value>472910740</value>
            </list>
        </constructor-arg>
    </bean>

    <bean class="top.yzlin.yyh.SRPK"
          c:groupID="${SR.groupID}"
          c:cqRobot-ref="cqSR">
        <constructor-arg name="proList">
            <list value-type="java.util.List">
                <list value-type="java.lang.Integer">
                    <value>42004</value>
                </list>
                <list value-type="java.lang.Integer">
                    <value>42005</value>
                </list>
                <list value-type="java.lang.Integer">
                    <value>42006</value>
                </list>
                <list value-type="java.lang.Integer">
                    <value>42008</value>
                </list>
                <list value-type="java.lang.Integer">
                    <value>42009</value>
                </list>
            </list>
        </constructor-arg>
    </bean>

</beans>